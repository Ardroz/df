doctype 5
html
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    title= title
    //if lt IE 9
      script(src='https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js')
      script(src='https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js')

    block stylesheets
      link(rel='stylesheet', href='/stylesheets/bootstrap/bootstrap.css')
      link(rel='stylesheet', href='/stylesheets/own/index.css')
    block custom-stylesheets
  body
    block content

    include footer

    block scripts

      script(src='/javascripts/jquery/jquery.min.js')
      script(src='/javascripts/jquery/jquery-ui-1.10.3.custom.min.js')
      script( src='/javascripts/angular/angular.min.js' )
      script(src='/javascripts/bootstrap/bootstrap.js')
      script(src='/javascripts/bootstrap/application.js')
      script(src='/javascripts/bootstrap/modal.js')
      script(src='/javascripts/bootstrap/carousel.js')

    block custom-scripts